# OpenAI Settings
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIM=1536

# Qdrant Settings
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_CHILD_COLLECTION=child_chunks
QDRANT_PARENT_COLLECTION=parent_chunks

# Retrieval Settings
RETRIEVAL_TOP_K=50
RERANK_TOP_K=5
HYBRID_DENSE_WEIGHT=0.7
HYBRID_SPARSE_WEIGHT=0.3

# Chunking Settings
PARENT_CHUNK_SIZE=2000
PARENT_CHUNK_OVERLAP=200
CHILD_CHUNK_SIZE=400
CHILD_CHUNK_OVERLAP=50

# MySQL Settings
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=rag
MYSQL_PASSWORD=rag_password
MYSQL_DATABASE=rag_service
MYSQL_ROOT_PASSWORD=root_password

# Global Scheduler Settings
INGESTION_INTERVAL_MINUTES=60
INGESTION_ENABLED=true

# Slack Adapter Settings
SLACK_ENABLED=true
SLACK_SYNC_INTERVAL_MINUTES=0
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL_TYPES=public_channel,private_channel
SLACK_MESSAGE_LIMIT=200
SLACK_INCLUDE_THREADS=true

# Jira Adapter Settings
JIRA_ENABLED=true
JIRA_SYNC_INTERVAL_MINUTES=0
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token
JIRA_PROJECTS=
JIRA_JQL_FILTER=
JIRA_MAX_RESULTS=100

# Application Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000
INGESTION_PORT=8001

# Monitoring Settings
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin
